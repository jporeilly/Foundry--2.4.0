{
    "log-driver": "json-file",
    "log-opts": {
      "max-size": "10m",
      "max-file": "3"
    },
    "exec-opts": ["native.cgroupdriver=systemd"],
    "data-root": "/Docker-Registry/data",
    "insecure-registries":["ansible-controller.skytap.example:443","0.0.0.0/0"],
    "icc": false,
    "tls": true,
    "tlsverify": true,
    "tlscacert": "/etc/docker/ssl/ca.pem",
    "tlscert": "/etc/docker/ssl/ca.crt",
    "tlskey": "/etc/docker/ssl/ca.key",
    "default-ulimits": {
      "nofile": {
        "Name": "nofile",
        "Hard": 64000,
        "Soft": 64000
      }
    },
    "storage-driver": "overlay2",
    "storage-opts": [
      "overlay2.override_kernel_check=true"
    ],
    "mtu": 1450
  }